<% if !@html: %>
  <div class='output empty'>
    <div class='spacer'></div>
    <span class='message'><%= @options.emptyMessage or 'Click to add your content here' %></span>
  </div>
<% else: %>
  <div class='output'><%- @html %></div>
<% end %>

<textarea style='display:none'><%= @markup %></textarea>
