<% include_codemirror %>
<% activate_codemirror language: 'javascript', selector: '#code' %>

<% @errors = @library_version.errors %>

<%= form_for([:admin, @library, @library_version]) do |f| %>

  <div class="field">
    <%= f.label :code %><br />
    <%= f.text_area :code, :id => 'code' %>
  </div>
  <div class="field">
    <%= f.label :deprecated %><br />
    <%= f.check_box :deprecated %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


