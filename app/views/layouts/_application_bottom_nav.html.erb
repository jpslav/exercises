<div id="bottom-nav-frame">
  <div id="bottom-nav-content">

    <div id="bottom-nav-links">

      <%= link_to "Terms of Use", main_app.tou_path %>
      <%= image_tag "links_dash.png" %>
      
      <%= link_to "About", main_app.about_path %>
      <%= image_tag "links_dash.png" %>

      <%= link_to "Developers", main_app.developers_path %>
      <%= image_tag "links_dash.png" %>      

      <%= link_to "Contact Us", main_app.contact_path %>

      <% if false %>
      <% links = [ ["Terms of Use", nil],
                   ["About", nil],
                   ["Contact Us", nil] ] %>

      <ul>
      <% links.each_with_index do |link, ii| %>
        <li>
        <%= link_to link[1] do %>
          <span class='bottom-nav-link'>
            <%= link[0] %>
            <%= content_tag :em if ii != links.size-1 %>
          </span>
        <% end %>
        </li>
      <% end %>
      </ul>
      <% end %>

    </div>

  </div>
</div>